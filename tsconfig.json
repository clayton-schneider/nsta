{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "ignoreDeprecations": "5.0",
    "paths": {
      "@data/*": ["src/data/*"],
      "@img/*": ["src/img/*"],
      "@components/*": ["src/components/*"],
      "@sections/*": ["src/sections/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
